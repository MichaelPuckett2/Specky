<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Michael Puckett</Authors>
    <Company>TripleG3</Company>
    <Description>Attribute based dependency injection.</Description>
    <Copyright>2019</Copyright>
    <PackageProjectUrl>https://github.com/MichaelPuckett2/Specky</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MichaelPuckett2/Specky</RepositoryUrl>
    <RepositoryType>dotnet standard library</RepositoryType>
    <PackageTags>DI, Dependency Injection, Specky, Speck</PackageTags>
    <PackageReleaseNotes>Adding configurations to Specky with:
SpeckyConfigurationAttribute(string configurationName) (class | parameter)
SpeckyConfigurationParametersAttribute() (property)</PackageReleaseNotes>
    <AssemblyVersion>1.0.4.5</AssemblyVersion>
    <FileVersion>1.0.4.5</FileVersion>
    <Version>1.0.4.5</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseFile></PackageLicenseFile>
    <PackageLicenseExpression></PackageLicenseExpression>
  </PropertyGroup>

</Project>
